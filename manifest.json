{
    "manifest_version": 2,
  
    "name": "Dark Mode",
    "description": "change the current website theme to dark!",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "pp.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "tabs", "http://*/*", "https://*/*"
     ]
  }